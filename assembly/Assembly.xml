<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3 http://maven.apache.org/xsd/assembly-1.1.3.xsd">
	<id>Assembly</id>
	<formats>
		<format>jar</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<!-- <excludes> <exclude>commons-logging:commons-logging</exclude> <exclude>log4j:log4j</exclude> 
				</excludes> -->
		</dependencySet>
	</dependencySets>
	
	<fileSets>
		<fileSet>
			<directory>assembly/</directory>
			<outputDirectory>/</outputDirectory>
			<fileMode>0755</fileMode>
		</fileSet>
		<fileSet>
			<directory>${basedir}</directory>
			<includes>
				<include>*.png</include>
			</includes>
			<excludes>
				<exclude>Пример_1.png</exclude>
				<exclude>Пример.png</exclude>
			</excludes>
		</fileSet>
	</fileSets>
	
	<files>
		<file>
			<source>Пример_1.png</source>
			<outputDirectory>/MyFiles_1</outputDirectory>
			<filtered>true</filtered>
		</file>
		<file>
			<source>Пример.png</source>
			<outputDirectory>/MyFiles</outputDirectory>
			<filtered>true</filtered>
		</file>
	</files>
</assembly>
